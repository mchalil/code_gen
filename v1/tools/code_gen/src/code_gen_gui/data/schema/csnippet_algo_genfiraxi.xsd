<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="CodeSnippets" targetNamespace="http://schemas.microsoft.com/VisualStudio/2008/CodeSnippet" xmlns:mstns="http://schemas.microsoft.com/VisualStudio/2008/CodeSnippet" xmlns="http://schemas.microsoft.com/VisualStudio/2008/CodeSnippet" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="CodeSnippets" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="CodeSnippet">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Header" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Title" type="xs:string" minOccurs="0" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="Snippet" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Code" nillable="true" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:simpleContent msdata:ColumnName="Code_Text" msdata:Ordinal="1">
                          <xs:extension base="xs:string">
                            <xs:attribute name="Language" form="unqualified" type="xs:string" />
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="Format" form="unqualified" type="xs:string" />
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>